  "name": "gbarena",
  "version": "3.0.12",
  "scripts": {
    "ng": "ng",
    "start": "nx serve",
    "build": "nx build",
    "watch": "nx build --watch --configuration development",
    "analyze": "nx build --stats-json && webpack-bundle-analyzer dist/gbarena/stats.json",
    "test": "nx test",
    "build-staging": " node  ./node_modules/@angular/cli/bin/ng build -c=staging && git add . && git commit -m 'repush' && git push origin development",
    "build-prod": " node  ./node_modules/@angular/cli/bin/ng build -c=production && git add . && git commit -m 'repush' && git push origin production",
    "build-prod-clear": " node  ./node_modules/@angular/cli/bin/ng build -c=production --aot --output-hashing=all&& git add . && git commit -m 'repush' && git push origin production",
    "serve-hostcheck": "node --max_old_space_size=4048 ./node_modules/@angular/cli/bin/ng serve --port 1818 --live-reload false --host 0.0.0.0 ",
    "serve": "node --max_old_space_size=4048 ./node_modules/@angular/cli/bin/ng serve --port 1818 --live-reload false",
    "hmr": "nx s --hmr --port 1818",
    "hme-hostcheck": "nx s --hmr --port 1818 --host 0.0.0.0",
    "i18n:init": "ngx-translate-extract --input ./src --output ./src/assets/i18n/template.json --key-as-default-value --replace false --format json",
    "i18n:extract": "ngx-translate-extract --input ./src --output ./src/app/locales/en-US.json ./src/app/locales/ar-EG.json --format namespaced-json --replace false"
  },
